{{!-- <a name="destinos" id="destinos"> --}}
    <div class="container-fluid d-none d-md-block d-lg-block">
        <div class="viajes">
            {{#each viajes}}
            <div class="ImgV">
                <div class="card">
                    <img class="card-img-top" src="../images/{{img_path}}" alt="Card image cap">
                    <div class="card-body">
                        <h5 class="card-title">{{viaje}}<br></h5>
                        <div class="money">
                            <h3>{{precio}}€</h3>
                        </div>
                        <div class="ofer">
                            <h4>Ahorra 90 Euros</h4>
                        </div>
                    </div>
                </div>
            </div>
            {{/each}}
          <a class="enlace" href="/descripcion">Ver mas</a>
        </div>
    </div>
    <div class="container-fluid">
        <div class="row justify-content-center">
            <div class="col-12">
                <div id="carouselExampleControls" class="carousel slide d-md-none lg-block xl-block" data-ride="carousel">
                    <div class="carousel-inner">
                        <div class="carousel-item active">
                            <img class="d-block w-100" src="../images/beijing.JPG" alt="First slide">
                        </div>
                        {{#each viajes}}
                        <div class="carousel-item">
                            <img class="d-block w-100" src="../images/{{img_path}}" alt="{{viaje}}">
                            <div class="money">
                            <h3>{{precio}}€</h3>
                        </div>
                        <div class="ofer">
                            <h4>Ahorra 90 Euros</h4>
                        </div>
                        </div>
                        {{/each}}
                    </div>
                    <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
                        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                        <span class="sr-only">Previous</span>
                    </a>
                    <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
                        <span class="carousel-control-next-icon" aria-hidden="true"></span>
                        <span class="sr-only">Next</span>
                    </a>
                </div>
            </div>
        </div>
    </div>
